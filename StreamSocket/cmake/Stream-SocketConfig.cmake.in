get_filename_component(STREAMSOCK_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(STREAMSOCK_PROJECT_NAME Stream-Socket)
list(APPEND CMAKE_MODULE_PATH ${STREAMSOCK_CMAKE_DIR})
include(CMakeFindDependencyMacro)

list(REMOVE_AT CMAKE_MODULE_PATH -1)

if(NOT TARGET SSK::${STREAMSOCK_PROJECT_NAME})
  include("${STREAMSOCK_CMAKE_DIR}/${STREAMSOCK_PROJECT_NAME}Targets.cmake")
  set_property(TARGET SSK::${STREAMSOCK_PROJECT_NAME} APPEND PROPERTY VERSION  ${${STREAMSOCK_PROJECT_NAME}_VERSION})
endif()

set(${STREAMSOCK_PROJECT_NAME}_LIBRARIES ${STREAMSOCK_PROJECT_NAME})
